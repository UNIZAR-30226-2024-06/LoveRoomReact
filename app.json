{
  "expo": {
    "name": "LoveRoom",
    "slug": "LoveRoom",
    "version": "2.0.0",
    "orientation": "default",
    "icon": "./assets/logoLoveRoom.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoLoveRoom.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoLoveRoom.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.andreei19.LoveRoom"
    },
    "web": {
      "favicon": "./assets/logoLoveRoom.png"
    },
    "extra": {
      "eas": {
        "projectId": "9a04248a-7cc5-44bd-9288-8d8139a8a361"
      }
    }
  }
}
